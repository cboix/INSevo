With a threshold of 20 = read quality, we pick out the reads and print allele count, avg. quality, sequences used, not used, and total for divergence tests
awk '$0 ~/SY/ || $0 ~/NS/{qual= 0;count=0;no = 0;allele=0;for(i=9;i <=NF; i= i+2){a = i+1; if($i ~ /\./ || $a < 20){no = no +1}; if(($i == 1 || $i == 0) && $a >= 20){count=count+1;qual = qual + $a}; if($i == 1 && $a >= 20){allele = allele + $i;}}; print allele,qual/count,count,no,no+count,$8;}'  ~/db/DGRP/fullgenes/Akt1_full.vcf > ~/db/DGRP/fullgenes/Akt1_divSNP
awk '$0 ~/SY/ || $0 ~/NS/{qual= 0;count=0;no = 0;allele=0;for(i=9;i <=NF; i= i+2){a = i+1; if($i ~ /\./ || $a < 20){no = no +1}; if(($i == 1 || $i == 0) && $a >= 20){count=count+1;qual = qual + $a}; if($i == 1 && $a >= 20){allele = allele + $i;}}; print allele,qual/count,count,no,no+count,$8;}'  ~/db/DGRP/fullgenes/CAP_full.vcf > ~/db/DGRP/fullgenes/CAP_divSNP
awk '$0 ~/SY/ || $0 ~/NS/{qual= 0;count=0;no = 0;allele=0;for(i=9;i <=NF; i= i+2){a = i+1; if($i ~ /\./ || $a < 20){no = no +1}; if(($i == 1 || $i == 0) && $a >= 20){count=count+1;qual = qual + $a}; if($i == 1 && $a >= 20){allele = allele + $i;}}; print allele,qual/count,count,no,no+count,$8;}'  ~/db/DGRP/fullgenes/chico_full.vcf > ~/db/DGRP/fullgenes/chico_divSNP
awk '$0 ~/SY/ || $0 ~/NS/{qual= 0;count=0;no = 0;allele=0;for(i=9;i <=NF; i= i+2){a = i+1; if($i ~ /\./ || $a < 20){no = no +1}; if(($i == 1 || $i == 0) && $a >= 20){count=count+1;qual = qual + $a}; if($i == 1 && $a >= 20){allele = allele + $i;}}; print allele,qual/count,count,no,no+count,$8;}'  ~/db/DGRP/fullgenes/Cyp6a16Psi_full.vcf > ~/db/DGRP/fullgenes/Cyp6a16Psi_divSNP
awk '$0 ~/SY/ || $0 ~/NS/{qual= 0;count=0;no = 0;allele=0;for(i=9;i <=NF; i= i+2){a = i+1; if($i ~ /\./ || $a < 20){no = no +1}; if(($i == 1 || $i == 0) && $a >= 20){count=count+1;qual = qual + $a}; if($i == 1 && $a >= 20){allele = allele + $i;}}; print allele,qual/count,count,no,no+count,$8;}'  ~/db/DGRP/fullgenes/Cyp6t2Psi_full.vcf > ~/db/DGRP/fullgenes/Cyp6t2Psi_divSNP
awk '$0 ~/SY/ || $0 ~/NS/{qual= 0;count=0;no = 0;allele=0;for(i=9;i <=NF; i= i+2){a = i+1; if($i ~ /\./ || $a < 20){no = no +1}; if(($i == 1 || $i == 0) && $a >= 20){count=count+1;qual = qual + $a}; if($i == 1 && $a >= 20){allele = allele + $i;}}; print allele,qual/count,count,no,no+count,$8;}'  ~/db/DGRP/fullgenes/Cyp9f3Psi_full.vcf > ~/db/DGRP/fullgenes/Cyp9f3Psi_divSNP
awk '$0 ~/SY/ || $0 ~/NS/{qual= 0;count=0;no = 0;allele=0;for(i=9;i <=NF; i= i+2){a = i+1; if($i ~ /\./ || $a < 20){no = no +1}; if(($i == 1 || $i == 0) && $a >= 20){count=count+1;qual = qual + $a}; if($i == 1 && $a >= 20){allele = allele + $i;}}; print allele,qual/count,count,no,no+count,$8;}'  ~/db/DGRP/fullgenes/dm_full.vcf > ~/db/DGRP/fullgenes/dm_divSNP
awk '$0 ~/SY/ || $0 ~/NS/{qual= 0;count=0;no = 0;allele=0;for(i=9;i <=NF; i= i+2){a = i+1; if($i ~ /\./ || $a < 20){no = no +1}; if(($i == 1 || $i == 0) && $a >= 20){count=count+1;qual = qual + $a}; if($i == 1 && $a >= 20){allele = allele + $i;}}; print allele,qual/count,count,no,no+count,$8;}'  ~/db/DGRP/fullgenes/eIF-4E_full.vcf > ~/db/DGRP/fullgenes/eIF-4E_divSNP
awk '$0 ~/SY/ || $0 ~/NS/{qual= 0;count=0;no = 0;allele=0;for(i=9;i <=NF; i= i+2){a = i+1; if($i ~ /\./ || $a < 20){no = no +1}; if(($i == 1 || $i == 0) && $a >= 20){count=count+1;qual = qual + $a}; if($i == 1 && $a >= 20){allele = allele + $i;}}; print allele,qual/count,count,no,no+count,$8;}'  ~/db/DGRP/fullgenes/Flo-2_full.vcf > ~/db/DGRP/fullgenes/Flo-2_divSNP
