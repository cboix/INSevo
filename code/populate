#Setting all nonreads to 0, and NO threshold, for Tajima's D:
awk ' $0 ~/SY/{print "0",$0} $0 ~/NS/{print "1",$0}'  ~/db/DGRP/fullgenes/Akt1_full.vcf | tr . 0 > ~/db/DGRP/fullgenes/Akt1_SNP
