#!/bin/bash
awk ' BEGIN{RS=">[A-Z]";} 1 < NR && NR < 5{gsub("\n","");gsub(/([a-z <_]*>)/,""); print $0}'  ~/db/SimV2/alnMSY/3R/FBgn0010379.fa.anc | perl -F, -ane ' $,=" "; print split("",$F[0])' | awk ' BEGIN{split("",a)}{for(i = 1; i <=NF; i++){a[i]=sprintf("%s%s ",a[i],$i)}}END{for(i = 1; i <=NF;i++){print a[i]}}' > ~/project/divergence/Akt1_comp
