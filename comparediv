#!/bin/bash
awk ' BEGIN{RS=">[A-Z]";split("",a)} 1 < NR{gsub("\n","");gsub(/([a-zFBtr0-9 <_]*>)/,""); a[NR] = $0}END{for(i=1;i < NR;i++){printf("%s\n",a[i])}}'  ~/db/SimV2/alnMSY/3R/FBgn0010379.fa.anc | perl -F, -ane ' $,=" "; print split("",$F[0])' | awk ' BEGIN{split("",a);}{count = 0;for(i = 1; i <=NF; i++){a[i]=sprintf("%s%s ",a[i],$i)}}END{for(i = 1; i <=NF;i++){print a[i]}}' | awk ' BEGIN{split("",c);split("",a)}{for(i=4; i <= NF; i++){if($i !~ /-/){c[i] = c[i] + 1; if(length(s[i]) == 3 && s[i] !~ /ATG/){c[i] = 0}else{s[i] = sprintf("%s%s",s[i],$i)}}};a[NR] = $0}END{for(i=4; i <=NF; i++){d=(d?(c[d]>c[i]?d:i):i)}; printf("%s\n",d); for(i=1;i <=NR;i++){print a[i]}}' | awk ' NR == 1{a = $0}NR!=1{print $1,$2,$3,$a}' | awk ' ($4 !~ /-/) && ($4 !~ /X/)' > ~/project/divergence/Akt1.spliced
