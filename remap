liftOver ~/db/DGRP/fullgenes/gig.bed ~/db/LiftOver/dm3ToDroSim1.over.chain ~/db/DGRP/simulgenes/gig_sim.bed ~/db/DGRP/simulgenes/gig_del.bed
liftOver ~/db/DGRP/fullgenes/Ilp1.bed ~/db/LiftOver/dm3ToDroSim1.over.chain ~/db/DGRP/simulgenes/Ilp1_sim.bed ~/db/DGRP/simulgenes/Ilp1_del.bed
liftOver ~/db/DGRP/fullgenes/Ilp2.bed ~/db/LiftOver/dm3ToDroSim1.over.chain ~/db/DGRP/simulgenes/Ilp2_sim.bed ~/db/DGRP/simulgenes/Ilp2_del.bed
liftOver ~/db/DGRP/fullgenes/Ilp3.bed ~/db/LiftOver/dm3ToDroSim1.over.chain ~/db/DGRP/simulgenes/Ilp3_sim.bed ~/db/DGRP/simulgenes/Ilp3_del.bed
liftOver ~/db/DGRP/fullgenes/Ilp4.bed ~/db/LiftOver/dm3ToDroSim1.over.chain ~/db/DGRP/simulgenes/Ilp4_sim.bed ~/db/DGRP/simulgenes/Ilp4_del.bed
liftOver ~/db/DGRP/fullgenes/Ilp5.bed ~/db/LiftOver/dm3ToDroSim1.over.chain ~/db/DGRP/simulgenes/Ilp5_sim.bed ~/db/DGRP/simulgenes/Ilp5_del.bed
liftOver ~/db/DGRP/fullgenes/Ilp6.bed ~/db/LiftOver/dm3ToDroSim1.over.chain ~/db/DGRP/simulgenes/Ilp6_sim.bed ~/db/DGRP/simulgenes/Ilp6_del.bed
liftOver ~/db/DGRP/fullgenes/Ilp7.bed ~/db/LiftOver/dm3ToDroSim1.over.chain ~/db/DGRP/simulgenes/Ilp7_sim.bed ~/db/DGRP/simulgenes/Ilp7_del.bed
liftOver ~/db/DGRP/fullgenes/InR.bed ~/db/LiftOver/dm3ToDroSim1.over.chain ~/db/DGRP/simulgenes/InR_sim.bed ~/db/DGRP/simulgenes/InR_del.bed
