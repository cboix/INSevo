liftOver ~/db/DGRP/fullgenes/Tsc1.bed ~/db/LiftOver/dm3ToDroSim1.over.chain ~/db/DGRP/simulgenes/Tsc1_sim.bed ~/db/DGRP/simulgenes/Tsc1_del.bed

#To join deletion information with the remaps:
echo 'Joining w/ Deletions'
awk '{a=(NR%2==0?sprintf("%s %s %s %s",$1,a,a,$4):substr($0,2,3)); if(NR%2==0){print a};}' ~/db/DGRP/simulgenes/Akt1_del.bed >> ~/db/DGRP/simulgenes/Akt1_sim.bed
awk '{a=(NR%2==0?sprintf("%s %s %s %s",$1,a,a,$4):substr($0,2,3)); if(NR%2==0){print a};}' ~/db/DGRP/simulgenes/CAP_del.bed >> ~/db/DGRP/simulgenes/CAP_sim.bed
awk '{a=(NR%2==0?sprintf("%s %s %s %s",$1,a,a,$4):substr($0,2,3)); if(NR%2==0){print a};}' ~/db/DGRP/simulgenes/chico_del.bed >> ~/db/DGRP/simulgenes/chico_sim.bed
awk '{a=(NR%2==0?sprintf("%s %s %s %s",$1,a,a,$4):substr($0,2,3)); if(NR%2==0){print a};}' ~/db/DGRP/simulgenes/dm_del.bed >> ~/db/DGRP/simulgenes/dm_sim.bed
awk '{a=(NR%2==0?sprintf("%s %s %s %s",$1,a,a,$4):substr($0,2,3)); if(NR%2==0){print a};}' ~/db/DGRP/simulgenes/eIF-4E_del.bed >> ~/db/DGRP/simulgenes/eIF-4E_sim.bed
awk '{a=(NR%2==0?sprintf("%s %s %s %s",$1,a,a,$4):substr($0,2,3)); if(NR%2==0){print a};}' ~/db/DGRP/simulgenes/Flo-1_del.bed >> ~/db/DGRP/simulgenes/Flo-1_sim.bed
awk '{a=(NR%2==0?sprintf("%s %s %s %s",$1,a,a,$4):substr($0,2,3)); if(NR%2==0){print a};}' ~/db/DGRP/simulgenes/Flo-2_del.bed >> ~/db/DGRP/simulgenes/Flo-2_sim.bed
awk '{a=(NR%2==0?sprintf("%s %s %s %s",$1,a,a,$4):substr($0,2,3)); if(NR%2==0){print a};}' ~/db/DGRP/simulgenes/foxo_del.bed >> ~/db/DGRP/simulgenes/foxo_sim.bed
awk '{a=(NR%2==0?sprintf("%s %s %s %s",$1,a,a,$4):substr($0,2,3)); if(NR%2==0){print a};}' ~/db/DGRP/simulgenes/gig_del.bed >> ~/db/DGRP/simulgenes/gig_sim.bed
awk '{a=(NR%2==0?sprintf("%s %s %s %s",$1,a,a,$4):substr($0,2,3)); if(NR%2==0){print a};}' ~/db/DGRP/simulgenes/Ilp1_del.bed >> ~/db/DGRP/simulgenes/Ilp1_sim.bed
awk '{a=(NR%2==0?sprintf("%s %s %s %s",$1,a,a,$4):substr($0,2,3)); if(NR%2==0){print a};}' ~/db/DGRP/simulgenes/Ilp2_del.bed >> ~/db/DGRP/simulgenes/Ilp2_sim.bed
awk '{a=(NR%2==0?sprintf("%s %s %s %s",$1,a,a,$4):substr($0,2,3)); if(NR%2==0){print a};}' ~/db/DGRP/simulgenes/Ilp3_del.bed >> ~/db/DGRP/simulgenes/Ilp3_sim.bed
awk '{a=(NR%2==0?sprintf("%s %s %s %s",$1,a,a,$4):substr($0,2,3)); if(NR%2==0){print a};}' ~/db/DGRP/simulgenes/Ilp4_del.bed >> ~/db/DGRP/simulgenes/Ilp4_sim.bed
awk '{a=(NR%2==0?sprintf("%s %s %s %s",$1,a,a,$4):substr($0,2,3)); if(NR%2==0){print a};}' ~/db/DGRP/simulgenes/Ilp5_del.bed >> ~/db/DGRP/simulgenes/Ilp5_sim.bed
