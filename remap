awk '{b =(length($4)>length($5)?length($4):length($5));a = sprintf("chr%s %s %s %s",$1,$2,$2+b,NR); print a}' ~/db/DGRP/fullgenes/dm_full.vcf > ~/db/DGRP/fullgenes/dm.bed
