#!/bin/bash
echo 'Creating bed files' 
awk '{b =(length($4)>length($5)?length($4):length($5));a = sprintf("chr%s %s %s %s",$1,$2,$2+b,NR); print a}' ~/db/DGRP/fullgenes/Akt1_full.vcf > ~/db/DGRP/fullgenes/Akt1.bed
awk '{b =(length($4)>length($5)?length($4):length($5));a = sprintf("chr%s %s %s %s",$1,$2,$2+b,NR); print a}' ~/db/DGRP/fullgenes/CAP_full.vcf > ~/db/DGRP/fullgenes/CAP.bed
