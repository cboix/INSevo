awk '{b =(length($4)>length($5)?length($4):length($5));a = sprintf("chr%s %s %s %s",$1,$2,$2+b,NR); print a}' ~/db/DGRP/fullgenes/sima_full.vcf > ~/db/DGRP/fullgenes/sima.bed
awk '{b =(length($4)>length($5)?length($4):length($5));a = sprintf("chr%s %s %s %s",$1,$2,$2+b,NR); print a}' ~/db/DGRP/fullgenes/Sin1_full.vcf > ~/db/DGRP/fullgenes/Sin1.bed
awk '{b =(length($4)>length($5)?length($4):length($5));a = sprintf("chr%s %s %s %s",$1,$2,$2+b,NR); print a}' ~/db/DGRP/fullgenes/SNF1A_full.vcf > ~/db/DGRP/fullgenes/SNF1A.bed
