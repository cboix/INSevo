awk '{a=(NR%2==0?sprintf("%s %s %s %s",$1,a,a,$4):substr($0,2,3)); if(NR%2==0){print a};}' ~/db/DGRP/simulgenes/Ilp7_del.bed >> ~/db/DGRP/simulgenes/Ilp7_sim.bed
awk '{a=(NR%2==0?sprintf("%s %s %s %s",$1,a,a,$4):substr($0,2,3)); if(NR%2==0){print a};}' ~/db/DGRP/simulgenes/InR_del.bed >> ~/db/DGRP/simulgenes/InR_sim.bed
awk '{a=(NR%2==0?sprintf("%s %s %s %s",$1,a,a,$4):substr($0,2,3)); if(NR%2==0){print a};}' ~/db/DGRP/simulgenes/lkb1_del.bed >> ~/db/DGRP/simulgenes/lkb1_sim.bed
awk '{a=(NR%2==0?sprintf("%s %s %s %s",$1,a,a,$4):substr($0,2,3)); if(NR%2==0){print a};}' ~/db/DGRP/simulgenes/Pdk1_del.bed >> ~/db/DGRP/simulgenes/Pdk1_sim.bed
