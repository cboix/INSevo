liftOver ~/db/DGRP/fullgenes/raptor.bed ~/db/LiftOver/dm3ToDroSim1.over.chain ~/db/DGRP/simulgenes/raptor_sim.bed ~/db/DGRP/simulgenes/raptor_del.bed
liftOver ~/db/DGRP/fullgenes/Rheb.bed ~/db/LiftOver/dm3ToDroSim1.over.chain ~/db/DGRP/simulgenes/Rheb_sim.bed ~/db/DGRP/simulgenes/Rheb_del.bed
liftOver ~/db/DGRP/fullgenes/rictor.bed ~/db/LiftOver/dm3ToDroSim1.over.chain ~/db/DGRP/simulgenes/rictor_sim.bed ~/db/DGRP/simulgenes/rictor_del.bed
liftOver ~/db/DGRP/fullgenes/RpS6.bed ~/db/LiftOver/dm3ToDroSim1.over.chain ~/db/DGRP/simulgenes/RpS6_sim.bed ~/db/DGRP/simulgenes/RpS6_del.bed
liftOver ~/db/DGRP/fullgenes/S6k.bed ~/db/LiftOver/dm3ToDroSim1.over.chain ~/db/DGRP/simulgenes/S6k_sim.bed ~/db/DGRP/simulgenes/S6k_del.bed
