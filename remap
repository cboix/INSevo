awk '{b =(length($4)>length($5)?length($4):length($5));a = sprintf("chr%s %s %s %s",$1,$2,$2+b,NR); print a}' ~/db/DGRP/fullgenes/Pten_full.vcf > ~/db/DGRP/fullgenes/Pten.bed
awk '{b =(length($4)>length($5)?length($4):length($5));a = sprintf("chr%s %s %s %s",$1,$2,$2+b,NR); print a}' ~/db/DGRP/fullgenes/RagA-B_full.vcf > ~/db/DGRP/fullgenes/RagA-B.bed
awk '{b =(length($4)>length($5)?length($4):length($5));a = sprintf("chr%s %s %s %s",$1,$2,$2+b,NR); print a}' ~/db/DGRP/fullgenes/RagC-D_full.vcf > ~/db/DGRP/fullgenes/RagC-D.bed
