awk '{b =(length($4)>length($5)?length($4):length($5));a = sprintf("chr%s %s %s %s",$1,$2,$2+b,NR); print a}' ~/db/DGRP/fullgenes/step_full.vcf > ~/db/DGRP/fullgenes/step.bed
awk '{b =(length($4)>length($5)?length($4):length($5));a = sprintf("chr%s %s %s %s",$1,$2,$2+b,NR); print a}' ~/db/DGRP/fullgenes/Thor_full.vcf > ~/db/DGRP/fullgenes/Thor.bed
awk '{b =(length($4)>length($5)?length($4):length($5));a = sprintf("chr%s %s %s %s",$1,$2,$2+b,NR); print a}' ~/db/DGRP/fullgenes/Tor_full.vcf > ~/db/DGRP/fullgenes/Tor.bed
awk '{b =(length($4)>length($5)?length($4):length($5));a = sprintf("chr%s %s %s %s",$1,$2,$2+b,NR); print a}' ~/db/DGRP/fullgenes/Tsc1_full.vcf > ~/db/DGRP/fullgenes/Tsc1.bed

#Find simulans remaps for necessary genes
echo 'Using LiftOver to remap' 
liftOver ~/db/DGRP/fullgenes/Akt1.bed ~/db/LiftOver/dm3ToDroSim1.over.chain ~/db/DGRP/simulgenes/Akt1_sim.bed ~/db/DGRP/simulgenes/Akt1_del.bed
liftOver ~/db/DGRP/fullgenes/CAP.bed ~/db/LiftOver/dm3ToDroSim1.over.chain ~/db/DGRP/simulgenes/CAP_sim.bed ~/db/DGRP/simulgenes/CAP_del.bed
liftOver ~/db/DGRP/fullgenes/chico.bed ~/db/LiftOver/dm3ToDroSim1.over.chain ~/db/DGRP/simulgenes/chico_sim.bed ~/db/DGRP/simulgenes/chico_del.bed
